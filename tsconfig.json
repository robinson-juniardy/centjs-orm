{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["ES2021.String", "DOM"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "rootDir": "./src",
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true
  }
}
